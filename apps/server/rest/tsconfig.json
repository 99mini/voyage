{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "strictPropertyInitialization": false,
    "resolveJsonModule": true,
    "types": ["jest"],
    "rootDir": "./src",
    "outDir": "./dist",
    "module": "CommonJS",
    "target": "ES2020",
    "baseUrl": ".",
    "paths": {
      "@server-rest/*": ["./src/*"]
    }
  },
  "include": ["./src/**/*"],
  "exclude": ["./dist/**/*"]
}
