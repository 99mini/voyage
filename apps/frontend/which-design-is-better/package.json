{
  "name": "which-design-is-better",
  "private": true,
  "version": "0.1.0",
  "sideEffects": false,
  "type": "module",
  "homepage": "https://which-design-is-better.zerovoyage.com",
  "scripts": {
    "build": "vite build",
    "build:ssg": "pnpm tsx ./.scripts/build-html.ts",
    "build:static": "pnpm run build && pnpm run build:ssg",
    "deploy": "sh ./.scripts/deploy.sh",
    "dev": "vite",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "format": "prettier --write .",
    "start": "vite preview",
    "start:prod": "serve ./dist",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@packages/api-client": "workspace:*",
    "@packages/vds": "workspace:*",
    "lucide-react": "^0.469.0",
    "react-query": "^3.39.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.5.2",
    "d3": "^7.9.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.20",
    "@types/d3": "^7.4.3",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^6.1.6",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
